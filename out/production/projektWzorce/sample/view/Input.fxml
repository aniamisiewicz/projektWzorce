<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controller.Input">
   <children>
      <Label fx:id="question" layoutX="285.0" layoutY="124.0" text="Label" />
      <TextField fx:id="inputField" layoutX="221.0" layoutY="173.0" />
      <Button fx:id="answer" layoutX="165.0" layoutY="283.0" mnemonicParsing="false" onAction="#answerQuestion" text="Answer" />
      <Button fx:id="next" layoutX="381.0" layoutY="283.0" mnemonicParsing="false" onAction="#nextQuestion" text="Next" />
      <Label fx:id="alert" layoutX="286.0" layoutY="332.0" />
   </children>
</AnchorPane>
